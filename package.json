{
  "name": "sutro-client",
  "version": "3.0.1",
  "description": "A simple and sugary client for using sutro APIs.",
  "main": "dist/node.js",
  "browser": "dist/browser.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "sutro",
    "api",
    "rest",
    "http",
    "request",
    "resource"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contra/sutro-client.git"
  },
  "author": "Contra <yo@contra.io> (https://contra.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/contra/sutro-client/issues"
  },
  "homepage": "https://github.com/contra/sutro-client#readme",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">= 15"
  },
  "scripts": {
    "update-ky": "wget -O src/ky/index.js 'https://raw.githubusercontent.com/sindresorhus/ky/main/index.js'",
    "docs": "typedoc src/index.ts --theme minimal && gh-pages -d docs",
    "lint": "prettier --write .",
    "build": "npm run clean && tsc -b && cp -R src/ky dist/ky",
    "clean": "rimraf dist",
    "test": "mocha --require ts-node/register --recursive --reporter spec test/*.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.11",
    "@types/lodash.merge": "^4.6.6",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.35",
    "@types/url-join": "^4.0.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "express": "^4.16.3",
    "get-port": "^5.1.1",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.8",
    "mocha": "^8.0.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.0",
    "should": "^13.0.0",
    "sutro": "^6.0.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.19",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "lodash.merge": "^4.6.2",
    "node-fetch": "^3.0.0",
    "qs": "^6.5.2",
    "template-url": "^1.0.0",
    "url-join": "^4.0.0",
    "web-streams-polyfill": "^3.0.2",
    "whatwg-fetch": "^3.5.0"
  }
}
