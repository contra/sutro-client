{
  "name": "sutro-client",
  "version": "2.1.1",
  "description": "A simple and sugary client for using sutro APIs.",
  "main": "dist/node.js",
  "browser": "dist/browser.js",
  "keywords": [
    "sutro",
    "api",
    "rest",
    "http",
    "request",
    "resource"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contra/sutro-client.git"
  },
  "author": "Contra <yo@contra.io> (https://contra.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/contra/sutro-client/issues"
  },
  "homepage": "https://github.com/contra/sutro-client#readme",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">= 15"
  },
  "scripts": {
    "update-ky": "wget -O src/ky.js 'https://raw.githubusercontent.com/sindresorhus/ky/main/index.js' ",
    "build": "npm run clean && babel src --out-dir dist",
    "clean": "rimraf dist",
    "lint": "eslint src test *.js --fix",
    "test": "mocha --require @babel/register --recursive --reporter spec && npm run lint"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/register": "^7.9.0",
    "@stae/babel-browser": "^1.0.0",
    "@stae/linters": "^1.0.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "eslint": "^7.0.0",
    "eslint-cli": "^1.0.0",
    "express": "^4.16.3",
    "get-port": "^5.1.1",
    "mocha": "^8.0.0",
    "rimraf": "^3.0.0",
    "should": "^13.0.0",
    "sutro": "^5.0.0"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "core-js": "3",
    "lodash.merge": "^4.6.2",
    "node-fetch": "^2.6.1",
    "qs": "^6.5.2",
    "template-url": "^1.0.0",
    "url-join": "^4.0.0",
    "web-streams-polyfill": "^3.0.2",
    "whatwg-fetch": "^3.5.0"
  }
}
